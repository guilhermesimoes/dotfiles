[color]
	ui = auto
[core]
	whitespace = trailing-space,space-before-tab
	editor = vim
[alias]
	alias = config --get-regexp 'alias.*'
	amend = commit --amend
	co = checkout
	patch = add --patch
	st = status
	cont = rebase --continue
[push]
	default = current
